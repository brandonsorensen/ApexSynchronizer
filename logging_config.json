{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "standard": {
      "format": "%(asctime)s.%(msecs)03d:%(name)s %(levelname)-s: %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console_handler": {
      "level": "NOTSET",
      "formatter": "standard",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    },
    "apex_handler": {
      "level": "INFO",
      "formatter": "standard",
      "class": "logging.FileHandler",
      "filename": "apex_api.log",
      "mode": "a"
    },
    "ps_handler": {
      "level": "INFO",
      "formatter": "standard",
      "class": "logging.FileHandler",
      "filename": "powerschool.log",
      "mode": "a"
    },
    "error_handler": {
      "level": "ERROR",
      "formatter": "standard",
      "class": "logging.FileHandler",
      "filename": "error.log",
      "mode": "a"
    }
  },
  "loggers": {
      "": {
      "handlers": ["console_handler"],
      "level": "WARNING",
      "propagate": false
    },
    "apex_synchronizer.apex_synchronizer": {
      "handlers": ["console_handler", "apex_handler"],
      "level": "INFO",
      "propagate": false
    },
    "apex_synchronizer.apex_session": {
      "handlers": ["console_handler", "apex_handler"],
      "level": "INFO",
      "propagate": false
    },
    "apex_synchronizer.enrollment.ApexEnrollment": {
      "handlers": ["console_handler", "apex_handler"],
      "level": "INFO",
      "propagate": false
    },
    "apex_synchronizer.apex_data_models": {
      "handlers": ["console_handler", "apex_handler"],
      "level": "INFO",
      "propagate": false
    },
    "apex_synchronizer.ps_agent": {
      "handlers": ["console_handler", "ps_handler"],
      "level": "INFO",
      "propagate": false
    },
    "apex_synchronizer.enrollment.PSEnrollment": {
      "handlers": ["console_handler", "ps_handler"],
      "level": "INFO",
      "propagate": false
    },
    "__main__": {
      "handlers": ["console_handler"],
      "level": "NOTSET",
      "propagate": false
    }
  }
}
